# .env
# Environment variables for the Financial Dashboard

# Required for actual Dhan API calls (Phase 1/2)
DHAN_ACCESS_TOKEN=YOUR_ACTUAL_DHAN_ACCESS_TOKEN
DHAN_CLIENT_ID=YOUR_DHAN_CLIENT_ID

# Optional: Override default database URL if needed
# DATABASE_URL=postgresql+psycopg2://your_db_user:your_db_password@timescaledb:5432/your_db_name

# Optional: Override CORS origins (comma-separated)
# BACKEND_CORS_ORIGINS=http://localhost:3000,https://your-deployed-frontend.com